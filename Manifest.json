{
    "manifest_version": 2,
    "name": "OSCAR OMSCS Filter",
    "description": "Filter to show OMSCS courses only on oscar",
    "version": "1.0",
    "author": "karthikarun@gatech.edu",
    "content_scripts": [
        {
          "matches": [
            "https://oscar.gatech.edu/pls/bprod/bwskfcls.P_GetCrse_Advanced*"
          ],
          "js": ["jquery-3.5.1.min.js", "filter.js"]
        }
      ],
    "browser_action": {
        "default_icon": "icon.ico"
    },
    "icons": { "16": "icon.ico",
        "48": "icon.ico",
       "128": "icon.ico" }
}